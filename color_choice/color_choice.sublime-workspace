                var numberRows = data.getNumberOfRows();\n                        data.removeRows(0, numberRows);\n                        data.addRows([\n                                                                                [new Date('2018-02-11'), 120000, customTooltip(new Date('2018-02-11'), '120000')],\n                                                ]);\n                        var tics = [];\n                        // for (var i = 2; i > 0; i--){\n                            for (var j = -11; j <= 11; j++) {\n                                tics.push({\n                                    v: getMonthCurrentData(j),\n                                    f: getNameMonth(j)\n                                });\n                            }\n                        // }\n                        var options = {\n                            pointSize: 15,\n                            colors: ['#FF4C4E'],\n                            legend: 'none',\n                            tooltip: { isHtml: true },\n                            vAxis: {\n                                format: 'short'\n                            },\n                            hAxis: {\n                                ticks: tics\n                            }\n                        };\n                        chart.draw(data, options);\n                    });\n                    function getMonthCurrentData(month) {\n                        var getYear = new Date().getFullYear();\n                        return new Date(getYear, month);\n                    }\n                    function getNameMonth(numberMonth) {\n                        switch (numberMonth) {\n                            case 0: return 'Янв';\n                            case 1: return 'Фев';\n                            case 2: return 'Мар';\n                            case 3: return 'Апр';\n                            case 4: return 'Май';\n                            case 5: return 'Июн';\n                            case 6: return 'Июл';\n                            case 7: return 'Авг';\n                            case 8: return 'Сент';\n                            case 9: return 'Окт';\n                            case 10: return 'Ноя';\n                            case 11: return 'Дек';\n                            case -11: return 'Фев';\n                            case -10: return 'Мар';\n                            case -9: return 'Апр';\n                            case -8: return 'Май';\n                            case -7: return 'Июн';\n                            case -6: return 'Июл';\n                            case -5: return 'Авг';\n                            case -4: return 'Сент';\n                            case -3: return 'Окт';\n                            case -2: return 'Ноя';\n                            case -1: return 'Дек';\n                        }\n                    }\n                    function dateInCurrentMonth(currentDay) {\n                        var currentYear = new Date().getFullYear();\n                        var currentMonth = new Date().getMonth();\n                        return new Date(currentYear, currentMonth, currentDay);\n                    }\n                    function resultMonthInDate(serialNumberMonth) {\n                        var currentYear = new Date().getFullYear();\n                        var currentMonth = new Date().getMonth();\n                        var prevMonth = currentMonth - 1;\n                        switch (serialNumberMonth) {\n                            case 0:\n                                return new Date(currentYear, currentMonth);\n                            case 1:\n                                prevMonth = checkMonthOnSerialNumber(prevMonth);\n                                return new Date(currentYear, prevMonth);\n                            case 2:\n                                prevMonth = checkMonthOnSerialNumber(prevMonth - 1);\n                                currentYear = checkMonthOnLastNumberInYear(prevMonth, currentYear);\n                                return new Date(currentYear, prevMonth);\n                            case 3:\n                                prevMonth = checkMonthOnSerialNumber(prevMonth - 2);\n                                currentYear = checkMonthOnLastNumberInYear(prevMonth, currentYear);\n                                return new Date(currentYear, prevMonth);\n                            case 4:\n                                prevMonth = checkMonthOnSerialNumber(prevMonth - 3);\n                                currentYear = checkMonthOnLastNumberInYear(prevMonth, currentYear);\n                                return new Date(currentYear, prevMonth);\n                            case 5:\n                                prevMonth = checkMonthOnSerialNumber(prevMonth - 4);\n                                currentYear = checkMonthOnLastNumberInYear(prevMonth, currentYear);\n                                return new Date(currentYear, prevMonth);\n                        }\n                    }\n                    function checkMonthOnSerialNumber(prevMonth) {\n                        if (prevMonth >= 0) {\n                            return prevMonth;\n                        }\n                        else {\n                            return 11;\n                        }\n                    }\n                    function checkMonthOnLastNumberInYear(prevMonth, currentYear) {\n                        if (prevMonth == 11) {\n                            return currentYear - 1;\n                        }\n                        else {\n                            return currentYear;\n                        }\n                    }\n                    function resultStrokeMonth(serialNumberMonth) {\n                        var currentDate = resultMonthInDate(serialNumberMonth);\n                        var currentMonth = currentDate.getMonth();\n                        return getNameMonth(currentMonth);\n                    }\n                }\n                function customTooltip(date, text) {\n                    var year = date.getFullYear();\n                    var month = date.getMonth();\n                    var day = date.getDate();\n                    return '<div class=\"detail-chart__tooltip\">' +\n                        '<div class=\"detail-chart__tooltip-text detail-chart__tooltip-text--padding\">' + year + '.' + (month + 1) + '.' + day + '<div>' +\n                        '<div class=\"detail-chart__tooltip-text\">$ ' + text + '</div>' +\n                        '</div>';\n                }\n                if (document.getElementById('chart')) {\n                    google.charts.load('current', {'packages': ['corechart']})\n                    google.charts.setOnLoadCallback(drawChart);\n                    // google_charts_1.GoogleCharts.load(drawChart, 'geochart');\n                }\n            }) \n        </script>\t\t\t            <div class=\"request\">\n            <div class=\"request__item-wrp request__item-wrp--contacts\">\n                <div class=\"request__item request__item--1\">\n                    <div class=\"request__img\" style=\"background-image: url(/img/qewafasda12dwds.jpg);\"></div>\n                </div>\n                <div class=\"request__item request__item--2\">\n                    <div class=\"request__header\">Илья Прусикин</div>\n                    <div class=\"request__text request__text--gray\">Агент</div>\n                    <a class=\"request__phone-number\" href=\"\">+7 (495) 000-00-00</a>\n\t\t\t\t</div>\n            </div>\n\t\t\t\n<form class=\"request__item-wrp request__item-wrp--form\"  name=\"SIMPLE_FORM_5\" action=\"/zagorodnaya/arenda/apartment/apartamenty_v_posyelke_1_oe_uspenskoe_shosse_id_15505-9239/\" method=\"POST\" enctype=\"multipart/form-data\"><input type=\"hidden\" name=\"sessid\" id=\"sessid\" value=\"34dcfe4d934d73e59257d9630e23ea99\" /><input type=\"hidden\