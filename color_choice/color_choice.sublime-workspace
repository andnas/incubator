   colors: ['#FF4C4E'],\n                            legend: 'none',\n                            tooltip: { isHtml: true },\n                            vAxis: {\n                                format: 'short'\n                            },\n                            hAxis: {\n                                ticks: tics\n                            }\n                        };\n                        chart.draw(data, options);\n                    });\n                    chartLinkHalfYear.addEventListener('click', function () {\n                        chartLinks.forEach(function (link) {\n                            link.classList.remove('detail-chart__text--active');\n                        });\n                        chartLinkHalfYear.classList.add('detail-chart__text--active');\n                        var numberRows = data.getNumberOfRows();\n                        data.removeRows(0, numberRows);\n                        data.addRows([\n                                                                    [new Date('2018-02-11'), 120000, customTooltip(new Date('2018-02-11'), '120000')],\n                                            ]);\n                        var tics = [];\n                        for (var i = 5; i >= 0; i--) {\n                            tics.push({\n                                v: resultMonthInDate(i),\n                                f: resultStrokeMonth(i)\n                            });\n                        }\n                        var options = {\n                            pointSize: 15,\n                            colors: ['#FF4C4E'],\n                            legend: 'none',\n                            tooltip: { isHtml: true },\n                            vAxis: {\n                                format: 'short'\n                            },\n                            hAxis: {\n                                ticks: tics\n                            }\n                        };\n                       